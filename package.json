{
  "name": "dmoc-web",
  "version": "1.0.0",
  "description": "DMOC Web (PWA) - Multi-tenant logistics operations app",
  "private": true,
  "scripts": {
    "dev": "npm run db:dev && node server.js",
    "dev:clean": "node scripts/dev-start.js",
    "dev:clean:win": "scripts\\dev-start.bat",
    "build": "npm run db:dev && prisma generate && next build",
    "start": "NODE_ENV=production node server.js",
    "vercel-build": "npm run db:dev && prisma generate && next build",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "format": "prettier --write .",
    "lint:fix": "next lint --fix",
    "changelog:add": "node scripts/changelog.js add",
    "changelog:version": "node scripts/changelog.js version",
    "changelog:generate": "node scripts/changelog.js generate",
    "changelog:current": "node scripts/changelog.js current",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "postinstall": "prisma generate",
    "db:dev": "node scripts/copy-schema.js prisma/schema-dev.prisma prisma/schema.prisma && prisma generate",
    "db:prod": "node scripts/copy-schema.js prisma/schema.prisma prisma/schema.prisma && prisma generate",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:prod": "tsx prisma/seed-production.ts",
    "deploy:prod": "npm run db:prod && npm run db:push && npm run db:seed:prod && npm run build",
    "dmoc:import:active": "tsx scripts/import/dmoc/index.ts --file=./data/active_manifests.txt",
    "dmoc:import:active:dry": "tsx scripts/import/dmoc/index.ts --file=./data/active_manifests.txt --dry-run"
  },
  "keywords": [
    "nextjs",
    "pwa",
    "logistics",
    "multi-tenant",
    "dmoc"
  ],
  "author": "Digiwize",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.910.0",
    "@aws-sdk/s3-request-presigner": "^3.914.0",
    "@prisma/client": "^5.0.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tanstack/react-query": "^4.36.0",
    "@trpc/client": "^10.45.0",
    "@trpc/next": "^10.45.0",
    "@trpc/react-query": "^10.45.0",
    "@trpc/server": "^10.45.0",
    "@types/leaflet": "^1.9.21",
    "bcrypt": "^5.1.0",
    "bullmq": "^4.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dexie": "^3.2.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.292.0",
    "minio": "^7.1.0",
    "next": "^14.0.0",
    "next-auth": "^4.24.0",
    "next-pwa": "^5.6.0",
    "node-fetch": "^3.3.2",
    "prisma": "^5.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-leaflet": "^4.2.1",
    "redis": "^4.6.0",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.0",
    "cpx2": "^8.0.0",
    "cross-env": "^10.1.0",
    "eslint": "^8.55.0",
    "eslint-config-next": "^14.0.0",
    "eslint-config-prettier": "^10.1.8",
    "postcss": "^8.4.0",
    "prettier": "^3.1.0",
    "tsx": "^4.20.6",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
