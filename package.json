{
  "name": "dmoc-web",
  "version": "1.0.0",
  "description": "DMOC Web (PWA) - Multi-tenant logistics operations app",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "build": "prisma generate && next build",
    "start": "NODE_ENV=production node server.js",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "format": "prettier --write .",
    "lint:fix": "next lint --fix",
    "changelog:add": "node scripts/changelog.js add",
    "changelog:version": "node scripts/changelog.js version",
    "changelog:generate": "node scripts/changelog.js generate",
    "changelog:current": "node scripts/changelog.js current",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "postinstall": "prisma generate",
    "db:dev": "cp prisma/schema-dev.prisma prisma/schema.prisma && prisma generate",
    "db:prod": "cp prisma/schema.prisma prisma/schema.prisma && prisma generate",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:prod": "tsx prisma/seed-production.ts",
    "deploy:prod": "npm run db:prod && npm run db:push && npm run db:seed:prod && npm run build"
  },
  "keywords": [
    "nextjs",
    "pwa",
    "logistics",
    "multi-tenant",
    "dmoc"
  ],
  "author": "Digiwize",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.910.0",
    "@prisma/client": "^5.0.0",
    "@tanstack/react-query": "^4.36.0",
    "@trpc/client": "^10.45.0",
    "@trpc/next": "^10.45.0",
    "@trpc/react-query": "^10.45.0",
    "@trpc/server": "^10.45.0",
    "@types/leaflet": "^1.9.21",
    "bcrypt": "^5.1.0",
    "bullmq": "^4.0.0",
    "dexie": "^3.2.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.292.0",
    "minio": "^7.1.0",
    "next": "^14.0.0",
    "next-auth": "^4.24.0",
    "next-pwa": "^5.6.0",
    "node-fetch": "^3.3.2",
    "prisma": "^5.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-leaflet": "^4.2.1",
    "redis": "^4.6.0",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.55.0",
    "eslint-config-next": "^14.0.0",
    "eslint-config-prettier": "^10.1.8",
    "postcss": "^8.4.0",
    "prettier": "^3.1.0",
    "tsx": "^4.20.6",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
